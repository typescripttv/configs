{
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "8.0.0",
    "@typescript-eslint/parser": "8.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-sort-keys-fix": "1.1.2"
  },
  "description": "Shareable ESLint Config",
  "devDependencies": {
    "eslint": "8.57.0"
  },
  "files": [
    ".eslintrc.json"
  ],
  "keywords": [
    "eslint",
    "config"
  ],
  "license": "MIT",
  "main": ".eslintrc.json",
  "name": "@tstv/eslint-config",
  "peerDependencies": {
    "eslint": "^8"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages",
    "type": "git",
    "url": "git+https://github.com/typescripttv/tsconfigs.git"
  },
  "scripts": {
    "build": "exit 0",
    "clean": "exit 0",
    "dist": "yarn clean && yarn build",
    "fix": "npm run test -- --fix",
    "test": "eslint -c ./.eslintrc.json --parser-options project:../tsconfig-common/tsconfig.json ../tsconfig-common/index.ts"
  },
  "version": "3.2.2"
}
